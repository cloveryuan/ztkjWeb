:global {
  .ant-layout-header{
    position: relative;
   
  }
  /*滚动条样式*/
  iframe::-webkit-scrollbar { /*滚动条整体样式*/
    width: 4px; /*高宽分别对应横竖滚动条的尺寸*/
    height: 4px;
  }
  iframe::-webkit-scrollbar-thumb { /*滚动条里面小方块*/
    -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.8);
    border-radius: 10px;
    background-color: gray;
  }
  iframe::-webkit-scrollbar-track { /*滚动条里面轨道*/
    border-radius: 10px;
    -webkit-box-shadow: inset 0 0 6px rgba(255, 255, 255, .8);
    background-color: #fff;
  }
  .top{
    padding: 0 !important;
    display:flex;
    align-items: center;
    justify-content: space-between;
     background:url('./../../image/bg.png') !important;
     background-size:100%;
    position: relative;
    .tit{
      color:#9b16c4;
      font-size:18px;
    }
    .center{
      color:#fff;
      width:160px;
      text-align:center;
      position: absolute;
      left:50%;
      margin-left:-80px;
    }
  }
  .ant-layout-sider-collapsed{
    width:60px!important;
    max-width: 60px;
    min-width :60px;
  }

}
