.box {
  position: relative;
  border-radius: 3px;
  background: #ffffff;
  border-top: 3px solid #3c8dbc;
  width: 100%;
  box-shadow: 0 1px 1px rgba(0,0,0,0.1);
  padding:0 10px;
  box-sizing:border-box;
}
.boxHeader {
  color: #444;
  display: block;
  padding: 5px;
  position: relative;
  border-bottom: 1px solid #f4f4f4;
}
.boxTitle {
  font-size: 18px;
  margin: 0;
  font-weight: 500;
}
.boxTools {
  position: absolute;
  right: 10px;
  top: 5px;
  float: right!important;
}
.btn {
  display: inline-block;
  margin-bottom: 0;
  font-weight: 400;
  text-align: center;
  white-space: nowrap;
  vertical-align: middle;
  touch-action: manipulation;
  user-select: none;
  padding: 5px;
  font-size: 12px;
  background: transparent;
  color: #97a0b3;
  border-radius: 3px;
  box-shadow: none;
  border: 1px solid transparent;
}
.boxBody {
  margin-top:6px;
  border-top-left-radius: 0;
  border-top-right-radius: 0;
  border-bottom-right-radius: 3px;
  border-bottom-left-radius: 3px;
}
:global{
  .ant-advanced-search-form {
    background: #fbfbfb;
    form{
      padding:5px;
    }
  }

  .ant-advanced-search-form .ant-form-item {
    display: flex;
  }

  .ant-advanced-search-form .ant-form-item-control-wrapper {
    flex: 1;
  }
  /*滚动条样式*/
  .ant-layout-content::-webkit-scrollbar {/*滚动条整体样式*/
    width: 4px;     /*高宽分别对应横竖滚动条的尺寸*/
    height: 4px;
  }
  .ant-layout-content::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.8);
    border-radius: 10px;
    background-color: gray;
  }
  .ant-layout-content::-webkit-scrollbar-track {/*滚动条里面轨道*/
    border-radius: 10px;
    -webkit-box-shadow: inset 0 0 6px rgba(255,255,255,.8);
    background-color: #fff;
  }
  .warntagColor0{
    color:#18BC9C;
  }
   .warntagColor1{
    color:#FF0000;
  }
   .warntagColor2{
    color:#808080;
  }
  .loadingpic{
  text-align: center;
  padding: 30px 50px;
  margin: 200px auto;
  box-sizing:border-box;
  }
  .optionsspin{
    width:100%;
    text-align:center;
    position:absolute;
    top:200px;
    margin:0 auto;
  }
  .ant-advanced-search-form .ant-form-item{
    margin-bottom:0!important
  }
  .ant-table-thead > tr > th{
    padding:8px 8px!important;
  }

  .ant-table-tbody > tr > td{
    padding:12px 8px!important
  }
  .editable-row-operations{
    color:#000!important;
  }
  .ant-table-thead th{
    text-align:center!important;
  }
  .ant-table-column-sorter{
     margin-left:0!important;
    margin-top:-20px!important;
  }
  .showcolumn{
    height:40px;
    position: relative;
  }
  .showcolumn .setback,.showcolumn .showLink{
    background:rgba(2, 6, 10,0.7);
    width:40px;
    height:40px;
    border-radius:10px;
    text-align:center;
    float:right;
    margin-left:15px;
  }
  .showLink .icon-lianjie{
    line-height: 40px;
    font-size:20px;
    color:#fff;
  }
  .showcolumn .anticon-menu-unfold{
    font-size:25px;
    color:#fff;
    line-height:40px;
  }
  .showcolumn .ant-form-item{
    margin-bottom:0!important
  }
  .showcolumn form h3{
    margin-bottom: 0!important
  }
  .showcolumn form{
    border-radius:6px;
    width:150px;
    border:1px solid #ccc;
    padding:10px;
    position:absolute;
    right:100px;
    z-index:999;
    background:rgba(2, 6, 10,0.7);
  }
}
table{
  tr{
    cursor:pointer;
     font-size:13px;
  }
}